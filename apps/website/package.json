    "@remix-run/node": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@remix-run/vercel": "^1.7.4",
    "@vercel/analytics": "^0.1.3",
    "@vercel/node": "^2.4.4",
    "clsx": "^1.2.1",
    "ioredis": "^5.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "remix-auth": "^3.3.0",
    "remix-auth-discord": "^1.0.0",
    "tailwind-merge": "^1.8.0",
    "tiny-invariant": "^1.3.1"
